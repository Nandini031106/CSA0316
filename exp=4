#include <stdio.h>
int main() {
    int n, i;
    int a = 0, b = 1, next;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    printf("fibonacci series: ");
    for (i = 1; i <= n; i++) {
        printf("%d ", a);
        next = a + b;
        a = b;
        b = next;
    }
    return 0;
}
output:
Enter the number of terms: 5
fibonacci series: 0 1 1 2 3 
